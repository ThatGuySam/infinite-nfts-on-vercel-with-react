{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { 
                "distDir": "dist" 
            }
        },
        {
            "src": "api/nft-media/index.js",
            "use": "@vercel/node",
            "config": { 
                "includeFiles": "src/assets/**" 
            }
        }
    ],
    "rewrites": [
        {
            "source": "/nft-media/:path*",
            "destination": "/api/nft-media"
        }
    ]
}